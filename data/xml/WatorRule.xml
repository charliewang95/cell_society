<?xml version="1.0" encoding="UTF-8"?>
<Rule id = "WatorRule">
    <nameRule>WatorRule</nameRule>
    <title>Wa-Tor</title>
    <author>Charlie</author>
    
    <cellLength>10</cellLength>
    <xSize>30</xSize>
    <ySize>20</ySize>
	<numNeighbor>3</numNeighbor>
	<numSide>3</numSide>
